    "@vercel/node": "^2.6.2",
    "axios": "^1.3.2",
    "axios-cookiejar-support": "^4.0.6",
    "http-cookie-agent": "^5.0.2",
    "ioredis": "^5.3.0",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tough-cookie": "^4.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "@types/axios": "^0.14.0",
